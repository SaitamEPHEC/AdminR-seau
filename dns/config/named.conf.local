acl internals {
    51.77.147.126;
    localhost;
};

view internal {
    match-clients { internals; };
    recursion yes;
    allow-recursion { internals; };
    zone "wt6.ephec-ti.be" {
        type master;
        file "/etc/bind/db.erp.wt6.ephec-ti.be";
    }
}

view external {

    match-clients { any; };

    zone "wt6.ephec-ti.be" IN {
        type master;
        file "/etc/bind/db.wt6.ephec-ti.be";
    };
};
